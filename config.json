{
    "protocols": {
        "description": "Routing Protocols",
        "type": "node",
        "bgp": {
            "script": "renderers/bgp.py",
            "address-family": {
                "description": "Lookup specific route",
                "type": "node",
                "<test>": {
                    "description": "Lookup specific route",
                    "type": "tagNode",
                    "validator": "enum",
                    "enum-values": ["ipv4-unicast", "ipv6-unicast"]
                }
            },
            "neighbor": {
                "description": "Lookup specific route",
                "type": "node",
                "<x.x.x.x>": {
                    "description": "Lookup specific route",
                    "type": "tagNode",
                    "validator": "ip-address",
                    "remote-as": {
                        "description": "Next hop for the static route",
                        "type": "node",
                        "<1-65535>": {
                            "description": "Lookup specific route",
                            "type": "tagNode",
                            "validator": "num-1-65535"
                        }
                    }
                }
            },
            "system-as": {
                "description": "Lookup specific route",
                "type": "node",
                "<1-65535>": {
                    "description": "Lookup specific route",
                    "type": "tagNode",
                    "validator": "num-1-65535"
                }
            }
        },
        "static": {
            "description": "Static Routing",
            "type": "node",
            "script": "renderers/static.py",
            "route": {
                "description": "Static IPv4 Route",
                "type": "node",
                "<x.x.x.x/x>": {
                    "description": "IPv4 Static Route",
                    "type": "tagNode",
                    "validator": "ip-prefix",
                    "multi": true,
                    "next-hop": {
                        "description": "Next-hop IPv4 router address",
                        "type": "node",
                        "<x.x.x.x>": {
                            "description": "Lookup specific route",
                            "type": "tagNode",
                            "validator": "ip-address",
                            "suggestor": "list_interfaces",
                            "suggestor_args": ["eth"],
                            "multi": true,
                            "distance": {
                                "description": "Distance for this route",
                                "type": "node",
                                "<1-255>": {
                                    "description": "Distance for this route",
                                    "type": "tagNode",
                                    "validator": "num-1-255"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "services": {
    "description": "Routing Protocols",
    "type": "node",
    "test": {
        "script": "renderers/test.py",
        "address-family": {
            "description": "Lookup specific route",
            "type": "node"
        }
    }
    }
}
